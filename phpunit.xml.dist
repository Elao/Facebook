<?xml version="1.0" encoding="UTF-8"?>

<phpunit bootstrap="./tests/bootstrap.php" colors="true">
    <testsuites>
        <testsuite name="Facebook SDK Test Suite">
            <directory suffix="Test.php">./tests/Facebook/Test/</directory>
        </testsuite>
    </testsuites>
    <filter>
        <whitelist>
            <directory suffix=".php">./src/Facebook/</directory>
        </whitelist>
    </filter>
</phpunit>
